<div>

	<h1>Venue</h1>
	<%= form_for(:session, url: login_path) do |f| %>	
		<h3>Sign In</h3>
		<div>
			<%= f.label :username %>
			<%= f.text_field :username %>
		</div>
		<div>
			<%= f.label :password %>
			<%= f.password_field :password %>
		<div>
		<div>
			<%= f.submit controller: :sessions, action: :create %>
		</div>
	<% end %>

	<h4>Want Access To #1 Rated Exclusives?</h4>
	<%= link_to "Sign Up!", new_user_path %>
</div>

<%= link_to "Contributors", signin_path %>